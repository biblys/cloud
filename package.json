  "bugs": {
    "url": "https://github.com/iwazaru/biblys-cloud/issues"
  },
  "homepage": "https://github.com/iwazaru/biblys-cloud#readme",
  "engines": {
    "node": ">=8.0"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^9.0.0",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^4.0.0",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.16.2",
    "moment": "^2.22.2",
    "mongoose": "^5.0.0",
    "morgan": "~1.9.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-stripe-elements": "^1.2.1",
    "request": "^2.83.0",
    "stripe": "^5.1.1",
    "twig": "1.10.5",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^3.0.0",
    "mocha": "^4.0.1",
    "mocha-eslint": "^4.1.0",
    "nodemon": "^1.12.1"
